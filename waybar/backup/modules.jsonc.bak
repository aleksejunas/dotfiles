{
  "clock": {
    "tooltip-format": "<big>{:%B %Y}</big>\n<tt><small>{calendar}</small></tt>",
    "format": ""
  },
  "clock#2": {
    "tooltip-format": "<big>{:%B %Y}</big>\n<tt><small>{calendar}</small></tt>",
    "format": "{:%I:%M %p}",
    "format-alt": "{:%a %b %d, %G}"
  },
  "modules-left": ["power-profiles-daemon"],
  "modules-center": ["memory"],
  "modules-right": ["clock"],
  "on-scroll-up-light": "light -A 5%",
  "on-scroll-down-light": "light -U 5%",
  "smooth-scrolling-threshold-light": 1,
  "backlight#2": {
    "on-scroll-up": "light -A 5%",
    "on-scroll-down": "light -U 5%",
    "smooth-scrolling-threshold": 1
  },
  "battery": {
    "good": 95,
    "warning": 30,
    "critical": 15,
    "format": "{icon}",
    "format-icons": ["", "", "", "", "", "", "", "", ""],
    "format-time": "{H}h {M}min",
    "tooltip": true
  },
  "battery#2": {
    "good": 95,
    "warning": 30,
    "critical": 15,
    "format": "{capacity}%",
    "format-alt": "{time}",
    "format-time": "{H}h {M}min",
    "tooltip": true
  },
  "bluetooth": {
    "tooltip-format-enumerate-connected": "{device_alias}\t{device_address}",
    "on-click": "~/.config/hypr/scripts/rofi_bluetooth",
    "on-click-right": "blueman-manager"
  },
  "bluetooth#2": {
    "tooltip-format-enumerate-connected": "{device_alias}\t{device_address}",
    "on-click": "~/.config/hypr/scripts/rofi_bluetooth",
    "on-click-right": "blueman-manager"
  },
  "cpu": {
    "interval": 5,
    "format": ""
  },
  "cpu#2": {
    "interval": 5,
    "format": "{usage}%"
  },
  "tooltip": false,
  "on-click-theme": "$HOME/.config/hypr/theme/theme.sh --pywal",
  "on-click-right-theme": "$HOME/.config/hypr/theme/theme.sh --default",
  "on-click-launcher": "$HOME/.config/hypr/scripts/rofi_launcher",
  "on-click-right-launcher": "$HOME/.config/hypr/scripts/rofi_runner",
  "disk": {
    "interval": 30,
    "format": ""
  },
  "disk#2": {
    "interval": 30,
    "format": "{free}"
  },
  "memory": {
    "interval": 10,
    "format": ""
  },
  "memory#2": {
    "interval": 10,
    "format": "{used:0.1f}G"
  },
  "on-scroll-up-playerctl": "playerctl position 05+",
  "on-scroll-down-playerctl": "playerctl position 05-",
  "smooth-scrolling-threshold-playerctl": 1,
  "on-scroll-up-mpc": "mpc seek +00:00:01",
  "on-scroll-down-mpc": "mpc seek -00:00:01",
  "smooth-scrolling-threshold-mpc": 1,
  "mpd#2": {
    "format-stopped": "玲",
    "on-click": "mpc prev"
  },
  "mpd#3": {
    "state-icons": {
      "paused": "",
      "playing": ""
    },
    "on-click": "mpc toggle"
  },
  "mpd#4": {
    "format-stopped": "怜",
    "on-click": "mpc next"
  },
  "tooltip-format": " {ifname} via {gwaddr}",
  "on-click-network": "~/.config/hypr/scripts/rofi_network",
  "network#2": {
    "format-disabled": "Disabled",
    "format-alt": " {bandwidthUpBits} |  {bandwidthDownBits}",
    "tooltip-format": " {ifname} via {gwaddr}"
  },
  "portable": "",
  "car": "",
  "volume-icons": ["", "", ""], // Renamed from "default"
  "scroll-step": 5.0,
  "on-click-pulsemixer": "pulsemixer --toggle-mute",
  "on-click-right-pulsemixer": "pulsemixer --toggle-mute",
  "smooth-scrolling-threshold-pulsemixer": 1,
  "pulseaudio#2": {
    "on-click": "pulsemixer --toggle-mute",
    "on-click-right": "pulsemixer --toggle-mute",
    "smooth-scrolling-threshold": 1
  },
  "format-icons": {
    "activated": "",
    "deactivated": ""
  },
  "timeout": 30,
  "urgent": "",
  "focused": "",
  "default-state": "", // Renamed from "default"
  "on-click": "activate",
  "tray": {
    "icon-size": 16,
    "spacing": 10
  },
  "power-profiles-daemon": {
    "performance": " ",
    "balanced": " ",
    "power-saver": " "
  }
}

